{
  "comments": false,
  "env": {
    "main": {
      "presets": [
        ["env", {
          "targets": { "node": 7 }
        }],
        "stage-0"
      ]
    },
    "renderer": {
      "presets": [
        ["env", {
          "modules": false
        }],
        "stage-0"
      ],
      "plugins": [
        "transform-runtime",
        "transform-decorators-legacy"
      ]
    },
    "web": {
      "presets": [
        ["env", {
          "modules": false
        }],
        "stage-0"
      ]
    },
    "test": {
      "presets": ["env"]
    }
  },
  "plugins": [
    "transform-runtime",
    "transform-decorators-legacy"
  ]
}
